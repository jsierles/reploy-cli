{
  "name": "reploy-cli",
  "version": "0.2.1",
  "description": "Manage React Native applications. Supports live updates.",
  "author": "Joshua Sierles",
  "main": "lib/reploy.js",
  "scripts": {
    "build": "rm -rf ./lib && node_modules/.bin/babel -d lib/ src/ && chmod 755 lib/*",
    "watch": "node_modules/.bin/babel -d lib/ src/ --watch",
    "pre-publish": "npm run build"
  },
  "dependencies": {
    "axios": "^0.9.1",
    "babel-cli": "^6.4.5",
    "bluebird": "^3.1.5",
    "cli": "^0.11.1",
    "commander": "2.9.0",
    "config-file": "^0.3.2",
    "form-data": "^1.0.0-rc3",
    "home-config": "^0.1.0",
    "node-progress": "^0.1.0",
    "os-homedir": "^1.0.1",
    "plist": "^1.2.0",
    "progress": "^1.1.8",
    "readline-sync": "^1.2.22",
    "reindex-js": "^0.3.1",
    "superagent": "visionmedia/superagent",
    "superagent-bluebird-promise": "^3.0.0",
    "superagent-promise": "^1.1.0",
    "timeago": "^1.5.0",
    "validator": "^4.5.1"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-plugin-syntax-async-functions": "^6.3.13",
    "babel-plugin-transform-async-to-module-method": "^6.4.0",
    "babel-plugin-transform-regenerator": "^6.4.4",
    "babel-polyfill": "^6.3.14",
    "babel-preset-async-to-bluebird": "^1.0.0",
    "babel-preset-es2015": "^6.3.13"
  },
  "bin": {
    "reploy": "lib/reploy.js"
  }
}
