{
  "name": "reploy-cli",
  "version": "1.0.0",
  "description": "Command line client for use with https://reploy.io. Upload, test and comment on React Native apps.",
  "author": "Joshua Sierles",
  "main": "lib/reploy.js",
  "scripts": {
    "build": "rm -rf ./lib && node_modules/babel-cli/bin/babel.js -d lib/ src/ && chmod 755 lib/*",
    "watch": "node_modules/babel-cli/bin/babel.js -d lib/ src/ --watch",
    "pre-publish": "npm run build"
  },
  "bin": {
    "reploy": "lib/reploy.js"
  },
  "devDependencies": {
    "axios": "^0.9.1",
    "babel-cli": "^6.6.5",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-3": "^6.5.0",
    "bluebird": "^3.3.4",
    "cli": "^0.11.2",
    "commander": "2.9.0",
    "config-file": "^0.3.2",
    "form-data": "^1.0.0-rc4",
    "home-config": "^0.1.0",
    "node-progress": "^0.1.0",
    "os-homedir": "^1.0.1",
    "plist": "^1.2.0",
    "progress": "^1.1.8",
    "readline-sync": "^1.4.1",
    "reindex-js": "^0.4.0",
    "superagent": "visionmedia/superagent",
    "superagent-bluebird-promise": "^3.0.0",
    "superagent-promise": "^1.1.0",
    "timeago": "^1.5.2",
    "validator": "^5.1.0"
  },
  "dependencies": {
    "babel-preset-stage-2": "^6.5.0",
    "fanoutpub": "^1.0.3",
    "optimist": "^0.6.1"
  }
}
