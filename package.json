{
  "name": "reploy-cli",
  "version": "1.2.2",
  "description": "Command line client for use with https://reploy.io. Upload, test and comment on React Native apps.",
  "author": "Joshua Sierles",
  "main": "lib/reploy.js",
  "scripts": {
    "build": "babel src --out-dir lib && chmod 755 lib/*",
    "watch": "node_modules/babel-cli/bin/babel.js -d lib/ src/ --watch",
    "pre-publish": "npm run build"
  },
  "bin": {
    "reploy": "lib/reploy.js"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-eslint": "6.0.2",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "eslint": "2.7.0",
    "eslint-config-standard-react": "2.3.0",
    "eslint-plugin-react": "^4.3.0",
    "eslint-plugin-standard": "1.3.2"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "bluebird": "^3.3.4",
    "cli": "^0.11.2",
    "commander": "2.9.0",
    "config-file": "^0.3.2",
    "fanoutpub": "^1.0.3",
    "form-data": "^1.0.0-rc4",
    "home-config": "^0.1.0",
    "lodash": "^4.9.0",
    "optimist": "^0.6.1",
    "os-homedir": "^1.0.1",
    "plist": "^1.2.0",
    "progress": "^1.1.8",
    "readline-sync": "^1.4.1",
    "reindex-js": "^0.4.0",
    "rollbar": "^0.6.0",
    "superagent": "1.8.3",
    "superagent-promise": "^1.1.0",
    "update-notifier": "^0.6.3",
    "validator": "^5.2.0"
  }
}
